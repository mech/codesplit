{
  "presets": [
    ["latest", { "es2015": { "modules": false } }],
    "react",
    "stage-0"
  ],

  "plugins": [
    "syntax-dynamic-import"
  ],

  "env": {
    "development": {
      "plugins": [
        "babel-plugin-transform-react-jsx-self",
        "babel-plugin-transform-react-jsx-source"
      ]
    }
  }
}
